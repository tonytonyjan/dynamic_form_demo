<h1><%= Admin::Attendee.model_name.human %> <%= link_to t('crud.new'), new_admin_attendee_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_attendees %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::Attendee, :id, :user, :form %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_attendees.each do |admin_attendee| %>
      <tr>
        <%= td_for admin_attendee, :id, %i[user email], %i[form title] %>
        <%= td_action_for admin_attendee %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_attendees %>