<h1><%= Admin::Form.model_name.human %> <%= link_to t('crud.new'), new_admin_form_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_forms %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::Form, :id, :title, :description %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_forms.each do |admin_form| %>
      <tr>
        <%= td_for admin_form, :id, :title, :description %>
        <%= td_action_for admin_form %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_forms %>